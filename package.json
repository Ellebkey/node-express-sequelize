{
  "name": "node-express-sequelize",
  "version": "1.1.0",
  "description": "A Boilerplate application for building REST APIs using node, express and sequelize with ES6 with code coverage",
  "author": "Joel Barranco <hello@joelbarranco.io>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=10.15.0",
    "npm": ">=6.4.1",
    "yarn": ">=1.12.3"
  },
  "scripts": {
    "start": "node index.js",
    "start:debug": "cross-env DEBUG=express-mongoose:* yarn start",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "sequelize",
    "mysql",
    "postgresql",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "acl": "^0.4.11",
    "bcrypt": "^3.0.3",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "4.16.4",
    "express-winston": "3.0.1",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-nodemon": "^2.4.2",
    "helmet": "3.15.0",
    "http-status": "1.3.1",
    "joi": "14.3.1",
    "jsonwebtoken": "8.4.0",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "morgan": "1.9.1",
    "mysql2": "^1.6.4",
    "sequelize": "^4.42.0",
    "uglify-js": "^3.4.9",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "commitizen": "^3.0.5",
    "coveralls": "^3.0.0",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "gulp-mocha": "^6.0.0",
    "husky": "1.3.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.2.0",
    "supertest": "3.3.0",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "gulp eslint"
    }
  }
}
